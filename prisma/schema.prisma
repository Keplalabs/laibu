// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("SB_DATABASE_URL")
}
model User {
  uid       String    @id 

  email   String   @unique

  username    String @unique
  
  year    Int?

  course String?

  role String?

  semester  Int?

  registeredUnits String[]
  
  moderatingUnits String[]
}
